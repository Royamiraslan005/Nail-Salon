@model NailSalon.Core.ViewModels.MasterVm

<form method="post" enctype="multipart/form-data">
    <div class="form-group">
        <label asp-for="FullName">FullName</label>
        <input asp-for="FullName" class="form-control">
        <span asp-validation-for="FullName"></span>
    </div>
    <div class="form-group">
        <label asp-for="Experience">Experience</label>
        <input asp-for="Experience" class="form-control">
        <span asp-validation-for="Experience"></span>
    </div>
    <div class="form-group">
        <label asp-for="Zodiac">Zodiac</label>
        <input asp-for="Zodiac" class="form-control">
        <span asp-validation-for="Zodiac"></span>
    </div>
    <div class="form-group">
        <label asp-for="Specialty">Specialty</label>
        <input asp-for="Specialty" class="form-control">
        <span asp-validation-for="Specialty"></span>
    </div>
    <div class="form-group">
        <label asp-for="formFile">formFile</label>
        <input asp-for="formFile" class="form-control">
        <span asp-validation-for="formFile"></span>
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>